@model IEnumerable<MvcShop.Models.ViewModels.Orders.OrderVM>

@{
    ViewBag.Title = "Orders";
}

<h2>Orders</h2>


<table class="table">
    <tr>              
        <th>
            ProductInfo
        </th>
        <th>
            DeliveryInfo
        </th>
        <th>
            Created At
        </th>
        <th>
            Delivery Status
        </th>      
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>        
        <td>
            @Html.DisplayFor(modelItem => item.ProductInfo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeliveryInfo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedAt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DeliveryStatus)
        </td>        
    </tr>
}

</table>
